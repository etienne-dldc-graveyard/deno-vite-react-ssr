    "server:dev": "vite build --watch --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "esbuild-plugin-cache": "^0.2.9",
    "typescript": "^4.4.4",
    "vite": "^2.8.0-beta.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "deno-importmap": "^0.1.6",
    "glob": "^7.2.0",
    "vite-plugin-remove-exports": "^1.1.0"
  }
}
